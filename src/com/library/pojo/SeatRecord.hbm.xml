<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.library.pojo.SeatRecord" table="seat_record">
        <id name="recordId" type="java.lang.Integer" column="RECORD_ID">
            <generator class="native"/>
        </id>
        <property name="seatId" type="java.lang.String" column="SEAT_ID"/>
        <property name="uId" type="java.lang.String" column="USER"/>
        <property name="startTime" type="java.util.Date" column="START_TIME"/>
        <property name="leaveTime" type="java.util.Date" column="LEAVE_TIME"/>
        <property name="status" type="java.lang.Boolean" column="STATUS"/>
        <property name="isout" type="java.lang.Integer" column="ISOUT"/>
    </class>
</hibernate-mapping>